{
  "title": "Google Maps Scraper Input",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "searchTerm": {
      "title": "Search term",
      "type": "string",
      "description": "e.g. 'photographer', 'fitnessstudio', 't√§towierer'",
      "editor": "textfield"
    },
    "latitude": {
      "title": "Latitude",
      "type": "string",
      "default": "48.397660"
    },
    "longitude": {
      "title": "Longitude",
      "type": "string",
      "default": "9.993304"
    },
    "radiusKm": {
      "title": "Radius in Kilometern",
      "type": "integer",
      "default": 50
    },
    "limit": {
      "title": "Max. Anzahl an Ergebnissen",
      "type": "integer",
      "default": 10
    }
  },
  "required": ["searchTerm"]
}
