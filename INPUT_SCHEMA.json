{
  "title": "Google Maps Scraper",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "searchStringsArray": {
      "title": "Suchbegriffe",
      "type": "array",
      "description": "Begriffe, nach denen auf Google Maps gesucht werden soll.",
      "editor": "stringList"
    },
    "locationQuery": {
      "title": "Ort (optional)",
      "type": "string",
      "editor": "textfield",
      "description": "z. B. Ulm"
    },
    "latitude": {
      "title": "Breitengrad (optional)",
      "type": "string",
      "editor": "textfield",
      "description": "z. B. 48.308727"
    },
    "longitude": {
      "title": "Längengrad (optional)",
      "type": "string",
      "editor": "textfield",
      "description": "z. B. 9.903306"
    },
    "radius": {
      "title": "Suchradius (in km)",
      "type": "integer",
      "editor": "number",
      "description": "z. B. 50"
    },
    "maxCrawledPlaces": {
      "title": "Maximale Anzahl Ergebnisse",
      "type": "integer",
      "editor": "number",
      "description": "z. B. 10"
    }
  },
  "required": [
    "searchStringsArray",
    "radius",
    "maxCrawledPlaces"
  ]
}
