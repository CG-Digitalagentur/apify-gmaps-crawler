{
  "title": "Google Maps Scraper Input",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "searchTerm": {
      "title": "Search term",
      "type": "string",
      "description": "z.B. 'fotograf', 'tätowierer', 'physiotherapeut'",
      "editor": "textfield"
    },
    "latitude": {
      "title": "Latitude",
      "type": "string",
      "description": "Breitengrad des Startpunkts",
      "editor": "textfield"
    },
    "longitude": {
      "title": "Longitude",
      "type": "string",
      "description": "Längengrad des Startpunkts",
      "editor": "textfield"
    },
    "radiusKm": {
      "title": "Radius in Kilometern",
      "type": "integer",
      "description": "Umkreis der Suche in km",
      "default": 50
    },
    "limit": {
      "title": "Anzahl der Ergebnisse",
      "type": "integer",
      "description": "Maximale Anzahl an Einträgen",
      "default": 10
    }
  },
  "required": ["searchTerm"]
}
